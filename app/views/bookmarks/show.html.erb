<p id="notice"><%= notice %></p>


<p>
  <b>Bookmarks Id :</b>
  <%= @bookmark.id %>
</p>

<p>
  <b>Bookmarks category:</b>

  <%  if @bookmark.bookmarks_category.nil? %>
    <td></td>
    <td></td>

<%  else %>
    <td><%= @bookmark.bookmarks_category.id %></td>
    <td><%= @bookmark.bookmarks_category.name  %></td>
<%  end %>


</p>

<p>
  <b>Item:</b>
  <%  if @bookmark.item.nil? %>
    <td></td>
    <td></td>

<%  else %>
    <td><%= @bookmark.item_id %></td>
    <td><%= @bookmark.item.name %></td>
<%  end %>


</p>

<p>
  <b>Bookmark url:</b>
  <%= @bookmark.bookmark_url.to_s %>
</p>

<p>
  <b>Title:</b>
  <%= @bookmark.title %>
</p>

<p>
  <b>I frame:</b>
  <%= @bookmark.i_frame %>
</p>

<p>
  <b>Image name:</b>
  <%= @bookmark.image_name %>
 <%= image_tag @bookmark.image_name.url.to_s %>
</p>

<p>
  <b>Image name desc:</b>
  <%= @bookmark.image_name_desc.to_s %>
  <%= image_tag @bookmark.image_name_desc.url.to_s %>
</p>

<p>
  <b>Description:</b>
  <%= @bookmark.description %>
</p>


<%= link_to 'Edit', edit_bookmark_path(@bookmark) %> |
<%= link_to 'Back', bookmarks_path %>

<div class="store_container">
  <div class="store_container_<%= this.type %> store-preview" id="store_<%= this.type %>_container_<%= this.id %>">
    <img class="store_image_cell" src="<%= this.image_name.url %>"></br>
    <% if this.name.trim().length > 30: %>
    <%= this.name.substr(0, 27).trim() + '...' %>
    <% else: %>
    <%= this.name.trim() %>
    <% end %>
    </br>
  </div>
</div>

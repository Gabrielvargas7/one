<ul class="browse_mode_list">
	<li><a id="browse_mode_active" href="#">Active</a></li>
	<li><a>Discover</a></li>
	<li><a>Bookmarks</a></li>
	<div class="spacer"></div>

	<%if @collection:%>
		<li id="browse_mode_list_selected"><img class="browse_mode_sidebar_icons" src="<%=@model.get('image_name').url%>"></li>
		<%for site in @collection:%>
		<li><img class="browse_mode_sidebar_icons" src="<%=site.get('image_name').url%>" data-id="<%=site.get('id')%>"></li>
		<%end%>
	<%end%>
</ul>
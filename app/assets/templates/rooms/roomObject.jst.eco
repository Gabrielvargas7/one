    <% for entry in @room_object: %>
        <div class="room_object_Container"
        style="position:absolute;
              width:<%=entry.width%>px;
              left:<%=entry.x%>px;
              top:<%=entry.y%>px;
              z-index:<%=entry.z%>;"><%= entry.name %>
        <img class="room_object" src="<%= entry.image_name.url%>">
        </div>
    <% end %>




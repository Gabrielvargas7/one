<h1>this view is on backbone -- Rooms</h1>


<%= @themes.length %>


<ul>
    <% for entry in @themes.models: %>

    <li><%= entry.get('id') %></li>
    <li><%= entry.get('name') %></li>
    <li><%= entry.get('description') %></li>

    <li><%= entry.get('image_name') %></li>
    <li><%= entry.get('image_name_selection') %></li>



    <% end %>
</ul>

